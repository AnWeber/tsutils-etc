{
  "author": "Nicholas Jamieson <nicholas@cartant.com>",
  "bugs": {
    "url": "https://github.com/cartant/tsutils-etc/issues"
  },
  "dependencies": {},
  "description": "More utils for use with tsutils",
  "devDependencies": {
    "@cartant/tslint-config": "^2.0.0",
    "@cartant/tslint-config-etc": "^2.0.0",
    "@phenomnomnominal/tsquery": "^4.0.0",
    "@types/chai": "^4.0.0",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.0",
    "chai": "^4.0.0",
    "cpy-cli": "^3.0.0",
    "husky": "^4.0.0",
    "lint-staged": "^10.0.0",
    "mkdirp": "^1.0.0",
    "mocha": "^8.0.0",
    "prettier": "^2.0.0",
    "rimraf": "^3.0.0",
    "ts-loader": "^8.0.0",
    "ts-node": "^8.0.0",
    "ts-snippet": "^4.0.0",
    "tslint": "^6.0.0",
    "tslint-etc": "^1.2.0",
    "tsutils": "^3.0.0",
    "typescript": "~3.9.3"
  },
  "es2015": "./dist/esm2015/index.js",
  "homepage": "https://github.com/cartant/tsutils-etc",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "tsutils",
    "typescript"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "git add"
    ]
  },
  "main": "./dist/index.js",
  "module": "./dist/esm5/index.js",
  "name": "tsutils-etc",
  "optionalDependencies": {},
  "peerDependencies": {
    "tsutils": "^3.0.0"
  },
  "private": true,
  "publishConfig": {
    "tag": "latest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cartant/tsutils-etc.git"
  },
  "scripts": {
    "dist": "yarn run dist:build && yarn run dist:copy",
    "dist:build": "yarn run dist:clean && yarn run dist:build:cjs && yarn run dist:build:esm2015 && yarn run dist:build:esm5",
    "dist:build:cjs": "tsc -p tsconfig-dist-cjs.json",
    "dist:build:esm2015": "tsc -p tsconfig-dist-esm2015.json",
    "dist:build:esm5": "tsc -p tsconfig-dist-esm5.json",
    "dist:clean": "rimraf dist",
    "dist:copy": "node scripts/pack.js && cpy CHANGELOG.md LICENSE README.md dist/",
    "lint": "tslint --project tsconfig.json source/**/*.ts",
    "prettier": "prettier --write \"./source/**/*.{js,json,ts}\"",
    "prettier:ci": "prettier --check \"./source/**/*.{js,json,ts}\"",
    "test": "yarn run lint && yarn run test:build && yarn run test:mocha",
    "test:build": "yarn run test:clean && tsc -p tsconfig.json",
    "test:clean": "rimraf build",
    "test:mocha": "mocha build/**/*-spec.js",
    "test:watch": "yarn run lint && yarn run test:build && karma start"
  },
  "types": "./dist/index.d.ts",
  "version": "1.2.2"
}
